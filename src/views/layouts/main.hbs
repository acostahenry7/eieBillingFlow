<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title></title>
    <link rel="stylesheet" href="/public/css/styles.css">
    <link rel="stylesheet" href="/public/css/outlets.css">

    <link rel="stylesheet" href="/public/css/dashboard.css">
    <link rel="stylesheet" href="/public/css/navbar.css">
    <link rel="stylesheet" href="/public/css/bootstrap/bootstrap.css">
    <link rel="stylesheet" href="/public/css/fontawesome/css/all.css">
    <link rel="stylesheet" href="/public/css/jquery/datatables/jquery.dataTable.css">
    <link rel="stylesheet" href="/public/css/datatables/rowReorder.bootstrap.css">
    <link rel="stylesheet" href="/public/css/datatables/responsive.dataTables.css">
    <link href="/public/css/datatables/select.dataTables.css" rel="stylesheet" type="text/css" />
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:wght@200&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Lobster&family=Nunito+Sans:wght@200&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:wght@200&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/public/css/products.css">
  </head>
  <body>
    <div id="all_content" class="container-fluid p-0">
      <div class="layout_content">
        {{> sidebar}}
        {{> companies/companiesForm}}
        {{> outlets/outletsForm}}
        {{> brands/brandsForm}}
        {{> models/modelsForm}}
        {{> variants/variantsForm}}
        {{> productTypes/productTypesForm}}
        {{> providers/providersForm}}
        {{> products/dealer/productsForm}}
        {{> products/dealer/comptp1ChoiceModal}}

        <div id="main" class="main">
            {{> navbar}}
            <div id="view_content" class="p-3">
                {{{body}}}
                <div id="main-spinner" class="spinner-border" role="status" hidden>
                  <span class="sr-only"  >Loading...</span>
                </div>
            </div>
        </div>
      </div>
      {{> warnings/createdWarning}}
      {{> warnings/removingConfirmation}}
      {{> warnings/disablingConfirmation}}
      {{> warnings/unauthorize}}
      {{> warnings/parentValidation}}
      {{> warnings/childValidation}}

    </div>




    <script src="/public/js/jquery/jquery.js"></script>
    <script src="/public/js/bootstrap/bootstrap.js" type="text/javascript"></script>
    <script src="/public/js/datatables/datatables.js" type="text/javascript"></script>
    <script src="/public/js/datatables/dataTables.rowReorder.js" type="text/javascript"></script>
    <script type="text/javascript" src="/public/js/datatables/dataTables.responsive.js"></script>
    <script src="/public/js/datatables/dataTables.select.js"></script>
    <script type="text/javascript" src="/public/js/animations/layoutAnimations.js"></script>
    <script type="text/javascript" src="/public/js/companies.js"></script>
    <script type="text/javascript" src="/public/js/outlets.js"></script>
    <script type="text/javascript" src="/public/js/brands.js"></script>
    <script type="text/javascript" src="/public/js/models.js"></script>
    <script type="text/javascript" src="/public/js/main.js"></script>
    <script type="text/javascript" src="/public/js/login.js"></script>
    <script type="text/javascript" src="/public/js/variants.js"></script>
    <script type="text/javascript" src="/public/js/productTypes.js"></script>
    <script type="text/javascript" src="/public/js/products/company_type1/products.js"></script>
    <script type="text/javascript" src="/public/js/providers.js"></script>
    <script type="text/javascript" src="/public/js/general.js"></script>
  </body>
</html>
